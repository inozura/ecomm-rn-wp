{
  "expo": {
    "name": "Zistou",
    "slug": "zistou",
    "version": "1.3.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#fb4349",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zistou",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture and upload user profile image and Listing Image.",
        "NSLocationWhenInUseUsageDescription": "This app usages device location on listing information, Search Data and User address."
      },
      "config": {
        "googleMapsApiKey": "AIzbSyCwx5yTQPJlVf04tUtMKPI1JGr4U6Bq9Yc"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.zistou",
      "config": {
        "googleMaps": {
          "apiKey": "AIzbSyCwx5yTQPJlVf04tUtMKPI1JGr4U6Bq9Yc"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "MEDIA_LIBRARY",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS",
        "LOCATION"
      ],
      "versionCode": 4
    }
  }
}
